{
  "version": 1,
  "projects": {
    "my-app": {
      "projectType": "application",
      "root": "apps/my-app",
      "sourceRoot": "apps/my-app/src",
      "prefix": "stackblitz-nx-angular",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/my-app",
            "index": "apps/my-app/src/index.html",
            "main": "apps/my-app/src/main.ts",
            "polyfills": "apps/my-app/src/polyfills.ts",
            "tsConfig": "apps/my-app/tsconfig.app.json",
            "assets": ["apps/my-app/src/favicon.ico", "apps/my-app/src/assets"],
            "styles": ["apps/my-app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/my-app/src/environments/environment.ts",
                  "with": "apps/my-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "my-app:build:production"
            },
            "development": {
              "browserTarget": "my-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "my-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-app/src/**/*.ts",
              "apps/my-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/my-app"],
          "options": {
            "jestConfig": "apps/my-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-app-e2e": {
      "root": "apps/my-app-e2e",
      "sourceRoot": "apps/my-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-app-e2e/cypress.json",
            "devServerTarget": "my-app:serve:development",
            "tsConfig": "apps/my-app-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/my-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "web-album-feature-detail": {
      "projectType": "library",
      "root": "libs/web/album/feature/detail",
      "sourceRoot": "libs/web/album/feature/detail/src",
      "prefix": "stackblitz-nx-angular",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web/album/feature/detail"],
          "options": {
            "jestConfig": "libs/web/album/feature/detail/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web/album/feature/detail/src/**/*.ts",
              "libs/web/album/feature/detail/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-album-feature-list": {
      "projectType": "library",
      "root": "libs/web/album/feature/list",
      "sourceRoot": "libs/web/album/feature/list/src",
      "prefix": "stackblitz-nx-angular",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web/album/feature/list"],
          "options": {
            "jestConfig": "libs/web/album/feature/list/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web/album/feature/list/src/**/*.ts",
              "libs/web/album/feature/list/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-album-feature-shell": {
      "projectType": "library",
      "root": "libs/web/album/feature/shell",
      "sourceRoot": "libs/web/album/feature/shell/src",
      "prefix": "as",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web/album/feature/shell"],
          "options": {
            "jestConfig": "libs/web/album/feature/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web/album/feature/shell/src/**/*.ts",
              "libs/web/album/feature/shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-shared-utils": {
      "root": "libs/web/shared/utils",
      "sourceRoot": "libs/web/shared/utils/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/web/shared/utils/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web/shared/utils"],
          "options": {
            "jestConfig": "libs/web/shared/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-shell-feature": {
      "projectType": "library",
      "root": "libs/web/shell/feature",
      "sourceRoot": "libs/web/shell/feature/src",
      "prefix": "as",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web/shell/feature"],
          "options": {
            "jestConfig": "libs/web/shell/feature/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web/shell/feature/src/**/*.ts",
              "libs/web/shell/feature/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-shell-ui-layout": {
      "projectType": "library",
      "root": "libs/web/shell/ui/layout",
      "sourceRoot": "libs/web/shell/ui/layout/src",
      "prefix": "as",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/web/shell/ui/layout"],
          "options": {
            "jestConfig": "libs/web/shell/ui/layout/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web/shell/ui/layout/src/**/*.ts",
              "libs/web/shell/ui/layout/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "defaultProject": "my-app",
  "schematics": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  }
}
